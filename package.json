{
  "name": "yveswetter-remix",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "react-router build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "cross-env NODE_ENV=development node ./server/dev-server.js",
    "start": "cross-env NODE_ENV=production node .",
    "start:proxy": "run-p start proxy",
    "proxy": "tsx ./other/reverse-proxy.ts",
    "typecheck": "react-router typegen && tsc -b",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/server-build"
  ],
