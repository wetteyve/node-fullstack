    "@radix-ui/react-slot": "^1.1.0",
    "@react-router/express": "7.6.0",
    "@react-router/node": "7.6.0",
    "address": "2.0.3",
    "axios": "1.9.0",
    "class-variance-authority": "0.7.0",
    "close-with-grace": "2.2.0",
